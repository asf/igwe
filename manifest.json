{
  "applications": {
    "gecko": {
      "id": "ig_download@example.com",
      "strict_min_version": "42.0",
      "update_url": "https://asf-nc.hopto.org/igwe/updates.json"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "css": [
        "content.css"
      ],
      "js": [
        "igcs.js"
      ],
      "matches": [
        "*://*.instagram.com/*"
      ]
    }
  ],
  "default_locale": "en",
  "description": "Download your favorite fotos and videos from Instagram.",
  "manifest_version": 2,
  "name": "IG Downloads",
  "options_ui": {
    "browser_style": true,
    "page": "options.html"
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "downloads",
    "notifications",
    "storage",
    "tabs"
  ],
  "version": "1.5",
  "web_accessible_resources": [
    "floppy-o.png"
  ]
}
